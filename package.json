{
  "name": "flourish-web-sdk-react",
  "version": "1.0.4",
  "description": "Flourish React Web SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "awesome-project"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flourish-savings/flourish-web-sdk-react.git"
  },
  "author": "Flourish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Flourish-savings/flourish-web-sdk-react/issues"
  },
  "homepage": "https://github.com/Flourish-savings/flourish-web-sdk-react#readme",
  "dependencies": {
    "@types/react": "^18.0.27",
    "react": "^18.2.0",
    "react-native-event-listeners": "^1.0.7",
    "react-native-webview": "^11.26.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
